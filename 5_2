#include <stdio.h>
#include <stdlib.h>

int main(){ 
    printf("Please enter n ");
    int n,j; 
    scanf("%d", &n);
    int a[n];
    printf("\nPlease enter the permutation ");
    for(j=0;j<n;j++){
        scanf("%d",&a[j]);
    }
    printf("\nInverse permutation ");
    int b[n];
    for(j=0;j<n;j++){
        b[a[j]-1]=j+1;
    }
    for(j=0;j<n;j++){
        printf("%d ",b[j]);
    }
return 0;
}
